<%=form_with model: @student do |f|%>
    <%=f.label "First Name: "%>
    <%=f.text_field :first_name%>
    <br>
    <%=f.label "Last Name: "%>
    <%=f.text_field :last_name%>
    <br>
    <%=f.label "Age: "%>
    <%=f.text_field :age%>
    <br>
    <%=f.label "Gender: "%>
    <%=f.text_field :gender%>
    <br>
    <%=f.label "Grade Level: "%>
    <%=f.text_field :grade_level%>
    <br>
    <br>
    <%=f.label "Courses: "%><br>
    <%=f.collection_check_boxes :course_ids, @courses, :id, :name%>
    
<br>
<br>

    <%=f.submit%>
<%end%>